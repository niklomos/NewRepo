@model IEnumerable<TestAdminLTE1.Models.Student>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@


@{
    ViewData["Title"] = "Test Model";
}

<h1>Test Model Page</h1>

<div>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Fname</td>
                    <td>@item.Lname</td>
                </tr>
            }

            <h2>@Model.ElementAt(0).Fname</h2>
        </tbody>
    </table>
</div>
