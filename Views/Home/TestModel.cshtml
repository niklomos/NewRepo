@model IEnumerable<string>

@{
    ViewData["Title"] = "Test Model";
}

<h1>Test Model Page</h1>

<div>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                var data = item.Split(',');
                <tr>
                    <td>@data[0].Split(':')[1]</td>
                    <td>@data[1].Split(':')[1]</td>
                    <td>@data[2].Split(':')[1]</td>
                </tr>
            }
        </tbody>
    </table>
</div>
